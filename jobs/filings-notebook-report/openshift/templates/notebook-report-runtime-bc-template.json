{
    "kind": "Template",
    "apiVersion": "v1",
    "metadata": {
        "name": "notebook-report-runtime-bc-template",
        "creationTimestamp": null
    },
    "objects": [
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "notebook-report-runtime",
                "creationTimestamp": null
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ImageChange",
                        "imageChange": {}
                    }
                ],
                "runPolicy": "Serial",
                "source": {
                    "type": "Dockerfile",
                    "dockerfile": "FROM gl2uos-tools/notebook-report:latest",
                    "images": [
                        {
                            "from": {
                                "kind": "ImageStreamTag",
                                "namespace": "openshift",
                                "name": "oracle-client-rpms:12.2"
                            },
                            "paths": [
                                {
                                    "sourcePath": "/tmp/oraclelibs",
                                    "destinationDir": "."
                                }
                            ]
                        }
                    ]
                },
                "strategy": {
                    "type": "Docker",
                    "dockerStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "notebook-report:latest"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "notebook-report-runtime:latest"
                    }
                },
                "resources": {},
                "postCommit": {},
                "nodeSelector": null
            },
            "status": {
                "lastVersion": 0
            }
        }
    ]
}
