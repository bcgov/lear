{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/annual_report",
    "required": [
        "annualReport"
    ],
    "type": "object",
    "title": "Annual Report Filing",
    "properties": {
        "annualReport": {
            "type": "object",
            "required": [
                "annualGeneralMeetingDate",
                "certifiedBy",
                "email"
            ],
            "properties": {
                "annualGeneralMeetingDate": {
                    "type": "string",
                    "format": "date"
                },
                "certifiedBy": {
                    "type": "string"
                },
                "email": {
                    "type": "string",
                    "format": "email"
                }
            }
        }
    }
}