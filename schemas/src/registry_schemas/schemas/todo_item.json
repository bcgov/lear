{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/todo_item",
    "required": [
        "todoItem"
    ],
    "type": "object",
    "title": "Todo Item",
    "properties": {
        "todoItem": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "title": "The type of this todo item.",
                    "examples": [
                        "annual_report"
                    ]
                },
                "ARFilingYear": {
                    "type": "integer",
                    "default": 0,
                    "examples": [
                        2019
                    ]
                }
            }
        }
    }
}